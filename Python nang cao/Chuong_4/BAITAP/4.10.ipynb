    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "# Tạo dữ liệu đầu vào\n",
    "ser = pd.Series(np.random.randint(1, 10, 7))\n",
    "\n",
    "# Tìm vị trí của các số là bội của 3\n",
    "indices = ser.index[ser % 3 == 0]\n",
    "\n",
    "# In kết quả\n",
    "print(\"Vị trí của các số là bội của 3 trong ser:\")\n",
    "print(indices)"
   ]
  }
